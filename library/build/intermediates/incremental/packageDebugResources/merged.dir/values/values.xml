    <style name="App_Theme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/dark_sky_blue</item>
        <item name="colorPrimaryVariant">@color/dark_sky_blue</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" ns1:targetApi="l">?attr/colorPrimaryVariant</item>

    </style>
    <style name="Home_Theme" parent="App_Theme">
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowTranslucentNavigation">false</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:statusBarColor">@color/dark_sky_blue</item>
        <item name="android:navigationBarColor" ns1:targetApi="27">@color/black</item>
        <item name="android:windowLightStatusBar" ns1:targetApi="23">true</item>
        <item name="android:windowLightNavigationBar" ns1:targetApi="28">false</item>
        <item name="android:windowBackground">@drawable/bg_splash</item>
    </style>
    <style name="ProgressDialogTheme" parent="ThemeOverlay.AppCompat.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <declare-styleable name="BaseButton">
        <attr name="button_type">
            <enum name="normal" value="0"/>
            <enum name="outlined" value="1"/>
            <enum name="text" value="2"/>
